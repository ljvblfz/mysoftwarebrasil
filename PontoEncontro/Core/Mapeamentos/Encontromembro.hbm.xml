<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Encontromembro, Core" table="[ENCONTROMEMBRO]" lazy="false">

		<composite-id name="Pk" class="Core.Encontromembro+PK, Core">
			<key-many-to-one name="Membroid" column= "[MEMBROID]" class="Core.Membro, Core"/>
			<key-many-to-one name="Encontroid" column= "[ENCONTROID]" class="Core.Encontro, Core"/>
		</composite-id>

		<property name="Encontromembrocriador" column= "[ENCONTROMEMBROCRIADOR]" type="String" length="1" not-null="true"/>


</class>
</hibernate-mapping>
