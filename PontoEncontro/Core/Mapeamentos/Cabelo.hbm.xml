<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Cabelo, Core" table="[CABELOS]" lazy="false">

		<id name="Cabelosid" column= "[CABELOSID]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Cabelosname" column= "[CABELOSNAME]" type="String" length="255" not-null="false"/>

		<bag name="PessoaList" inverse="true" lazy="false" cascade="all">
			<key column="[SEXOID]"/>
			<one-to-many class="Core.Pessoa"/>
		</bag>
		<bag name="SexoidList" inverse="true" lazy="false" cascade="all">
			<key column="[SEXOID]"/>
			<one-to-many class="Core.Pessoa"/>
		</bag>

</class>
</hibernate-mapping>
