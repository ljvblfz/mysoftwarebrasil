<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Perfil, Core" table="[PERFIL]" lazy="false">

		<id name="Perfilid" column= "[PERFILID]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Perfilaltura" column= "[PERFILALTURA]" type="int" not-null="true"/>
		<property name="Perfilpeso" column= "[PERFILPESO]" type="int" not-null="true"/>
		<property name="Perfilfumante" column= "[PERFILFUMANTE]" type="String" length="1" not-null="true"/>
		<property name="Perfilbebe" column= "[PERFILBEBE]" type="String" length="1" not-null="true"/>
		<property name="Pessoainteresseconhecer" column= "[PESSOAINTERESSECONHECER]" type="String" not-null="false"/>
		<property name="Pessoafantasiassexuai" column= "[PESSOAFANTASIASSEXUAIS]" type="String" not-null="false"/>
		<property name="Pessoaoutrosinteresse" column= "[PESSOAOUTROSINTERESSES]" type="String" not-null="false"/>

		<bag name="PessoaList" inverse="true" lazy="false" cascade="all">
			<key column="[SEXOID]"/>
			<one-to-many class="Core.Pessoa"/>
		</bag>
		<bag name="SexoidList" inverse="true" lazy="false" cascade="all">
			<key column="[SEXOID]"/>
			<one-to-many class="Core.Pessoa"/>
		</bag>

</class>
</hibernate-mapping>
