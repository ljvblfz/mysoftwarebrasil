<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Cidade, Core" table="[CIDADE]" lazy="false">

		<id name="Cidadeid" column= "[CIDADEID]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Cidadename" column= "[CIDADENAME]" type="String" length="255" not-null="true"/>
		<many-to-one name="Estadoid" column= "[ESTADOID]" class="Core.Estado, Core" not-null="true"/>

		<bag name="EnderecoList" inverse="true" lazy="false" cascade="all">
			<key column="[CIDADEID]"/>
			<one-to-many class="Core.Endereco"/>
		</bag>
		<bag name="CidadeidList" inverse="true" lazy="false" cascade="all">
			<key column="[CIDADEID]"/>
			<one-to-many class="Core.Endereco"/>
		</bag>

</class>
</hibernate-mapping>
