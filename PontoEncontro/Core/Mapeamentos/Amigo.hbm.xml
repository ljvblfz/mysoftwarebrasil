<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Amigo, Core" table="[AMIGOS]" lazy="false">

		<id name="Amigoid" column= "[AMIGOID]" type="int">
			<generator class="assigned" />
		</id>


		<bag name="MembroamigoList" inverse="true" lazy="false" cascade="all">
			<key column="[MEMBROID]"/>
			<one-to-many class="Core.Membroamigo"/>
		</bag>
		<bag name="MembroidList" inverse="true" lazy="false" cascade="all">
			<key column="[MEMBROID]"/>
			<one-to-many class="Core.Membroamigo"/>
		</bag>

</class>
</hibernate-mapping>
