<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Foto, Core" table="[FOTOS]" lazy="false">

		<composite-id name="Pk" class="Core.Foto+PK, Core">
			<key-many-to-one name="Membroid" column= "[MEMBROID]" class="Core.Membro, Core"/>
			<key-property name="Fotoid" column= "[FOTOID]" type="int"/>
		</composite-id>

		<property name="Fotopath" column= "[FOTOPATH]" type="String" length="255" not-null="true"/>

		<bag name="PostList" inverse="true" lazy="false" cascade="all">
			<key column="[MEMBROID]"/>
			<one-to-many class="Core.Post"/>
		</bag>
</class>
</hibernate-mapping>
