<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Pessoa, Core" table="[PESSOA]" lazy="false">

		<id name="Pessoaid" column= "[PESSOAID]" type="int">
			<generator class="assigned" />
		</id>

		<many-to-one name="Sexoid" column= "[SEXOID]" class="Core.Sexo, Core" not-null="true"/>
		<many-to-one name="Olhosid" column= "[OLHOSID]" class="Core.Olho, Core" not-null="true"/>
		<many-to-one name="Cabelosid" column= "[CABELOSID]" class="Core.Cabelo, Core" not-null="true"/>
		<many-to-one name="Etiniaid" column= "[ETINIAID]" class="Core.Etinia, Core" not-null="true"/>
		<many-to-one name="Perfilid" column= "[PERFILID]" class="Core.Perfil, Core" not-null="true"/>
		<many-to-one name="Estadocivilid" column= "[ESTADOCIVILID]" class="Core.Estadocivil, Core" not-null="true"/>
		<many-to-one name="Enderecoid" column= "[ENDERECOID]" class="Core.Endereco, Core" not-null="true"/>
		<property name="Pessoaname" column= "[PESSOANAME]" type="String" length="255" not-null="true"/>
		<property name="Pessoanascimento" column= "[PESSOANASCIMENTO]" type="DateTime" not-null="true"/>
		<property name="Pessoaprofissao" column= "[PESSOAPROFISSAO]" type="String" length="255" not-null="true"/>
		<property name="Pessoaemail" column= "[PESSOAEMAIL]" type="String" length="255" not-null="true"/>
		<property name="Pessoamsn" column= "[PESSOAMSN]" type="String" length="255" not-null="false"/>

		<bag name="MembroList" inverse="true" lazy="false" cascade="all">
			<key column="[PESSOAID]"/>
			<one-to-many class="Core.Membro"/>
		</bag>
		<bag name="InretessepessoaList" inverse="true" lazy="false" cascade="all">
			<key column="[PESSOAID]"/>
			<one-to-many class="Core.Inretessepessoa"/>
		</bag>
</class>
</hibernate-mapping>
