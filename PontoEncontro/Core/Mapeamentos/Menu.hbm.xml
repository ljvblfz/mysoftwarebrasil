<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Menu, Core" table="[MENU]" lazy="false">

		<id name="Menuid" column= "[MENUID]" type="int">
			<generator class="assigned" />
		</id>

		<many-to-one name="Menuidpai" column= "[MENUIDPAI]" class="Core.Menu, Core" not-null="true"/>
		<property name="Menuname" column= "[MENUNAME]" type="String" length="255" not-null="true"/>
		<property name="Menuordem" column= "[MENUORDEM]" type="int" not-null="true"/>
		<property name="Menupath" column= "[MENUPATH]" type="String" length="255" not-null="true"/>

		<bag name="MenuList" inverse="true" lazy="false" cascade="all">
			<key column="[MENUIDPAI]"/>
			<one-to-many class="Core.Menu"/>
		</bag>
		<bag name="MenuroleList" inverse="true" lazy="false" cascade="all">
			<key column="[ROLEID]"/>
			<one-to-many class="Core.Menurole"/>
		</bag>

</class>
</hibernate-mapping>
