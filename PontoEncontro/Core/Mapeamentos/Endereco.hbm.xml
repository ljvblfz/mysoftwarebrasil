<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="Core.Endereco, Core" table="[ENDERECO]" lazy="false">

		<id name="Enderecoid" column= "[ENDERECOID]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Cep" column= "[CEP]" type="String" length="20" not-null="false"/>
		<many-to-one name="Cidadeid" column= "[CIDADEID]" class="Core.Cidade, Core" not-null="true"/>

		<bag name="EncontroList" inverse="true" lazy="false" cascade="all">
			<key column="[ENDERECOID]"/>
			<one-to-many class="Core.Encontro"/>
		</bag>
		<bag name="EnderecoidList" inverse="true" lazy="false" cascade="all">
			<key column="[ENDERECOID]"/>
			<one-to-many class="Core.Encontro"/>
		</bag>
		<bag name="PessoaList" inverse="true" lazy="false" cascade="all">
			<key column="[SEXOID]"/>
			<one-to-many class="Core.Pessoa"/>
		</bag>
		<bag name="SexoidList" inverse="true" lazy="false" cascade="all">
			<key column="[SEXOID]"/>
			<one-to-many class="Core.Pessoa"/>
		</bag>

</class>
</hibernate-mapping>
