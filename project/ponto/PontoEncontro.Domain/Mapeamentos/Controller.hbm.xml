<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Controller, PontoEncontro.Domain" table="[CONTROLLER]" lazy="false">

		<id name="Idcontroller" column= "[IDCONTROLLER]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Namecontroller" column= "[NAMECONTROLLER]" type="String" length="255" not-null="false"/>

		<bag name="PermissoeList" inverse="true" lazy="false" cascade="all">
			<key column="[IDACTION]"/>
			<one-to-many class="PontoEncontro.Domain.Permissoe"/>
		</bag>
		<bag name="IdactionList" inverse="true" lazy="false" cascade="all">
			<key column="[IDACTION]"/>
			<one-to-many class="PontoEncontro.Domain.Permissoe"/>
		</bag>

</class>
</hibernate-mapping>
