<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Role, PontoEncontro.Domain" table="[ROLE]" lazy="false">

		<id name="Idrole" column= "[IDROLE]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Namerole" column= "[NAMEROLE]" type="String" length="255" not-null="true"/>

		<bag name="MembroList" inverse="true" lazy="false" cascade="all">
			<key column="[IDPESSOA]"/>
			<one-to-many class="PontoEncontro.Domain.Membro"/>
		</bag>
		<bag name="IdpessoaList" inverse="true" lazy="false" cascade="all">
			<key column="[IDPESSOA]"/>
			<one-to-many class="PontoEncontro.Domain.Membro"/>
		</bag>
		<bag name="PermissaoroleList" inverse="true" lazy="false" cascade="all">
			<key column="[IDROLE]"/>
			<one-to-many class="PontoEncontro.Domain.Permissaorole"/>
		</bag>
		<bag name="IdroleList" inverse="true" lazy="false" cascade="all">
			<key column="[IDROLE]"/>
			<one-to-many class="PontoEncontro.Domain.Permissaorole"/>
		</bag>

</class>
</hibernate-mapping>
