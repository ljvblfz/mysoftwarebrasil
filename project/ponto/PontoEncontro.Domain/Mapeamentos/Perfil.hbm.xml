<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Perfil, PontoEncontro.Domain" table="[PERFIL]" lazy="false">

		<id name="Idperfil" column= "[IDPERFIL]" type="int">
			<generator class="assigned" />
		</id>

		<many-to-one name="Idsexo" column= "[IDSEXO]" class="PontoEncontro.Domain.Sexo, PontoEncontro.Domain" not-null="false"/>
		<many-to-one name="Idolho" column= "[IDOLHO]" class="PontoEncontro.Domain.Olho, PontoEncontro.Domain" not-null="false"/>
		<many-to-one name="Idcabelo" column= "[IDCABELO]" class="PontoEncontro.Domain.Cabelo, PontoEncontro.Domain" not-null="false"/>
		<many-to-one name="Idetinia" column= "[IDETINIA]" class="PontoEncontro.Domain.Etinia, PontoEncontro.Domain" not-null="false"/>
		<many-to-one name="Idestadocivil" column= "[IDESTADOCIVIL]" class="PontoEncontro.Domain.Estadocivil, PontoEncontro.Domain" not-null="false"/>
		<many-to-one name="Idendereco" column= "[IDENDERECO]" class="PontoEncontro.Domain.Endereco, PontoEncontro.Domain" not-null="true"/>

		<bag name="PessoaList" inverse="true" lazy="false" cascade="all">
			<key column="[IDPERFIL]"/>
			<one-to-many class="PontoEncontro.Domain.Pessoa"/>
		</bag>
		<bag name="IdperfilList" inverse="true" lazy="false" cascade="all">
			<key column="[IDPERFIL]"/>
			<one-to-many class="PontoEncontro.Domain.Pessoa"/>
		</bag>
		<bag name="InteresseList" inverse="true" lazy="false" cascade="all">
			<key column="[IDTIPOINTERESSE]"/>
			<one-to-many class="PontoEncontro.Domain.Interesse"/>
		</bag>
		<bag name="IdtipointeresseList" inverse="true" lazy="false" cascade="all">
			<key column="[IDTIPOINTERESSE]"/>
			<one-to-many class="PontoEncontro.Domain.Interesse"/>
		</bag>
		<bag name="ContatoList" inverse="true" lazy="false" cascade="all">
			<key column="[IDPERFIL]"/>
			<one-to-many class="PontoEncontro.Domain.Contato"/>
		</bag>
</class>
</hibernate-mapping>
