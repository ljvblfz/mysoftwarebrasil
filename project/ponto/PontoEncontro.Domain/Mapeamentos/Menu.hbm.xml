<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Menu, PontoEncontro.Domain" table="[MENU]" lazy="false">

		<id name="Idmenu" column= "[IDMENU]" type="int">
			<generator class="assigned" />
		</id>

		<many-to-one name="Menuidpai" column= "[MENUIDPAI]" class="PontoEncontro.Domain.Menu, PontoEncontro.Domain" not-null="true"/>
		<property name="Namemenu" column= "[NAMEMENU]" type="String" length="255" not-null="true"/>
		<property name="Ordemmenu" column= "[ORDEMMENU]" type="int" not-null="true"/>
		<property name="Pathmenu" column= "[PATHMENU]" type="String" length="255" not-null="true"/>

		<bag name="MenuList" inverse="true" lazy="false" cascade="all">
			<key column="[MENUIDPAI]"/>
			<one-to-many class="PontoEncontro.Domain.Menu"/>
		</bag>
		<bag name="MenuidpaiList" inverse="true" lazy="false" cascade="all">
			<key column="[MENUIDPAI]"/>
			<one-to-many class="PontoEncontro.Domain.Menu"/>
		</bag>

</class>
</hibernate-mapping>
