<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Associado, PontoEncontro.Domain" table="[ASSOCIADO]" lazy="false">

		<id name="Idassociado" column= "[IDASSOCIADO]" type="int">
			<generator class="assigned" />
		</id>

		<many-to-one name="Idmembro" column= "[IDMEMBRO]" class="PontoEncontro.Domain.Membro, PontoEncontro.Domain" not-null="true"/>
		<many-to-one name="Idassociacao" column= "[IDASSOCIACAO]" class="PontoEncontro.Domain.Associacao, PontoEncontro.Domain" not-null="true"/>


</class>
</hibernate-mapping>
