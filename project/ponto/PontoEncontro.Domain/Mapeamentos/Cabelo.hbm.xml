<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Cabelo, PontoEncontro.Domain" table="[CABELO]" lazy="false">

		<id name="Idcabelo" column= "[IDCABELO]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Namecabelo" column= "[NAMECABELO]" type="String" length="255" not-null="false"/>

		<bag name="PerfilList" inverse="true" lazy="false" cascade="all">
			<key column="[IDSEXO]"/>
			<one-to-many class="PontoEncontro.Domain.Perfil"/>
		</bag>
		<bag name="IdsexoList" inverse="true" lazy="false" cascade="all">
			<key column="[IDSEXO]"/>
			<one-to-many class="PontoEncontro.Domain.Perfil"/>
		</bag>

</class>
</hibernate-mapping>
