<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Permissaorole, PontoEncontro.Domain" table="[PERMISSAOROLE]" lazy="false">

		<composite-id name="Pk" class="PontoEncontro.Domain.Permissaorole+PK, PontoEncontro.Domain">
			<key-many-to-one name="Idrole" column= "[IDROLE]" class="PontoEncontro.Domain.Role, PontoEncontro.Domain"/>
			<key-many-to-one name="Idpermissao" column= "[IDPERMISSAO]" class="PontoEncontro.Domain.Permissoe, PontoEncontro.Domain"/>
		</composite-id>



</class>
</hibernate-mapping>
