                    <div style="font: 25px times new roman; color: #FFFFFF; padding: 10px;">
                        @ViewBag.Title
                    </div>
                    <div style="padding: 10px;">
                        <div style="background-color: #FFFFFF;">
                            @Html.Partial("_LogOnPartial")
                        </div>
                    </div>
                </div>
            </div>
            <div style="width: 749px; color: #FFFFFF; padding: 20px; text-align: left;">
                @RenderBody()
            </div>
        </center>
    </div>
</body>
</html>
