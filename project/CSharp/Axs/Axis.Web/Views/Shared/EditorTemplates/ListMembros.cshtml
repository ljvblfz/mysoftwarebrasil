@model Axis.Infrastructure.Linq.Dynamic
@using Axis.Infrastructure
@using Axis.Infrastructure.IO
@{
    if (Model != null && Model.ListObjects.Count > 0)
    {
        foreach (Dictionary<Type, object> item in Model.ListObjects)
        {
            var membro = item[typeof(Axis.Domain.Membro)] as Axis.Domain.Membro;
            var photo = item[typeof(Axis.Domain.Foto)] as Axis.Domain.Foto;
            var img = Image.GetBase64Image(photo.pathFoto + "\\" + photo.nameFoto);
            <text>
             <div>
                <fieldset>
                    <div class="fieldLinha3">
                        <img src="data:image/gif;base64,@img" height="100" width="100" alt="" />
                    </div>
                    <div class="fieldLinha3">
                        <strong>@membro.loginMembro</strong>
                        <span>@Date.GetAge(membro.pessoa.nascimentoPessoa)</span><br />
                        <span>
                                @membro.pessoa.perfil.endereco.cidade.nameCidade ,
                                @membro.pessoa.perfil.endereco.cidade.estado.SiglaEstado  ,
                                Brasil
                        </span>                                                                  
                    </div>
                    <div class="fieldLinha3"></div>
                </fieldset>
             </div>
            </text>
        }
    }
}