<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Contato, PontoEncontro.Domain" table="[CONTATO]" lazy="false">

		<id name="Idcontato" column= "[IDCONTATO]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Valorcontato" column= "[VALORCONTATO]" type="String" length="255" not-null="true"/>
		<many-to-one name="Idperfil" column= "[IDPERFIL]" class="PontoEncontro.Domain.Perfil, PontoEncontro.Domain" not-null="true"/>
		<many-to-one name="Idtipocontato" column= "[IDTIPOCONTATO]" class="PontoEncontro.Domain.Tipocontato, PontoEncontro.Domain" not-null="true"/>


</class>
</hibernate-mapping>
