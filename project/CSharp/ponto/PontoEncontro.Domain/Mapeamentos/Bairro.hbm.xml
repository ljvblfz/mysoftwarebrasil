<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Bairro, PontoEncontro.Domain" table="[BAIRRO]" lazy="false">

		<id name="Idbairro" column= "[IDBAIRRO]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Nomebairro" column= "[NOMEBAIRRO]" type="String" length="255" not-null="true"/>

		<bag name="EnderecoList" inverse="true" lazy="false" cascade="all">
			<key column="[IDCIDADE]"/>
			<one-to-many class="PontoEncontro.Domain.Endereco"/>
		</bag>
		<bag name="IdcidadeList" inverse="true" lazy="false" cascade="all">
			<key column="[IDCIDADE]"/>
			<one-to-many class="PontoEncontro.Domain.Endereco"/>
		</bag>

</class>
</hibernate-mapping>
