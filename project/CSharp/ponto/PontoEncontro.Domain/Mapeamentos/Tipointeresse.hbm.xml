<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Tipointeresse, PontoEncontro.Domain" table="[TIPOINTERESSE]" lazy="false">

		<id name="Idtipointeresse" column= "[IDTIPOINTERESSE]" type="int">
			<generator class="assigned" />
		</id>

		<property name="Nametipointeresse" column= "[NAMETIPOINTERESSE]" type="String" length="255" not-null="true"/>

		<bag name="InteresseList" inverse="true" lazy="false" cascade="all">
			<key column="[IDTIPOINTERESSE]"/>
			<one-to-many class="PontoEncontro.Domain.Interesse"/>
		</bag>
		<bag name="IdtipointeresseList" inverse="true" lazy="false" cascade="all">
			<key column="[IDTIPOINTERESSE]"/>
			<one-to-many class="PontoEncontro.Domain.Interesse"/>
		</bag>

</class>
</hibernate-mapping>
