<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true">
	<class name="PontoEncontro.Domain.Associacao, PontoEncontro.Domain" table="[ASSOCIACAO]" lazy="false">

		<id name="Idassociacao" column= "[IDASSOCIACAO]" type="int">
			<generator class="assigned" />
		</id>

		<many-to-one name="Idtipoassociacao" column= "[IDTIPOASSOCIACAO]" class="PontoEncontro.Domain.Tipoassociacao, PontoEncontro.Domain" not-null="true"/>
		<many-to-one name="Idmembro" column= "[IDMEMBRO]" class="PontoEncontro.Domain.Membro, PontoEncontro.Domain" not-null="true"/>
		<many-to-one name="Idpessoa" column= "[IDPESSOA]" class="PontoEncontro.Domain.Pessoa, PontoEncontro.Domain" not-null="true"/>

		<bag name="AssociadoList" inverse="true" lazy="false" cascade="all">
			<key column="[IDMEMBRO]"/>
			<one-to-many class="PontoEncontro.Domain.Associado"/>
		</bag>
		<bag name="IdmembroList" inverse="true" lazy="false" cascade="all">
			<key column="[IDMEMBRO]"/>
			<one-to-many class="PontoEncontro.Domain.Associado"/>
		</bag>

</class>
</hibernate-mapping>
